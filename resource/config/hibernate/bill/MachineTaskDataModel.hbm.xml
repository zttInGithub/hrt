<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hrt.biz.bill.entity.model">
	<class name="MachineTaskDataModel" table="BILL_MACHINETASKDATA">
		<id name="bmtdID" column="BMTDID">
			<generator class="sequence">
				<param name="sequence">S_BILL_MACHINETASKDATA</param>
			</generator>
		</id>
		<property name="taskID" column="TASKID"/>
		<property name="unno" column="UNNO"/>
		<property name="mid" column="MID"/>
		<property name="type" column="TYPE"/>
		<property name="changeType"  column="CHANGETYPE"/>
		<property name="tid" column="TID"/>
		<property name="bmtID"  column="BMTID"/>
		<property name="bmaID" column="BMAID"/>
		<property name="machinesn" column="MACHINESN"/>
		<property name="simtel" column="SIMTEL"/>
		<property name="taskConfirmDate" type="timestamp" column="TASKCONFIRMDATE"/>
		<property name="proCessconText" column="PROCESSCONTEXT"/>
		<property name="maintainUid"  column="MAINTAINUID"/>
		<property name="maintainDate" type="timestamp"  column="MAINTAINDATE"/>
		<property name="maintainType"  column="MAINTAINTYPE"/>
		<property name="approveUid"  column="APPROVEUID"/>
		<property name="approveDate" type="timestamp"  column="APPROVEDATE"/>
		<property name="approveStatus"  column="APPROVESTATUS"/>
		<property name="taskStartDate" type="timestamp"  column="TASKSTARTDATE"/>
		<property name="taskStep1Date" type="timestamp"  column="TASKSTEP1DATE"/>
		<property name="taskStep2Date" type="timestamp"  column="TASKSTEP2DATE"/>
		<property name="remarks"  column="REMARKS"/>
    </class>
</hibernate-mapping>