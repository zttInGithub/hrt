<%@ page language="java" contentType="text/html; charset=UTF-8"
         pageEncoding="UTF-8" %>

<form id="sysAdmin_enum_modifyForm" method="post">
  <table class="table">
    <tr>
      <th>枚举类型：</th>
      <td>
        <select name="enumType" class="easyui-combobox easyui-validatebox" disabled="disabled" data-options="required:true" style="width:205px;">
          <option value="1" selected="selected">系统定义(SYS)</option>
          <option value="2">单表定义</option>
        </select>
      </td>
    </tr>
    <tr>
      <th>枚举表名：</th>
      <td><input name="enumTable" class="easyui-validatebox" data-options="required:true" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>枚举列名：</th>
      <td><input name="enumColumn" class="easyui-validatebox" data-options="required:true" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>枚举状态：</th>
      <td>
        <select name="enumStatus" class="easyui-validatebox" data-options="required:true"  style="width:205px;">
          <option value="1" selected="selected">开启</option>
          <option value="2">关闭</option>
        </select>
      </td>
    </tr>
    <tr>
      <th>枚举编码：</th>
      <td><input name="enumCode" class="easyui-validatebox" data-options="required:true" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>枚举描述：</th>
      <td><input name="enumDesc" class="easyui-validatebox" data-options="required:true" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>枚举值名称：</th>
      <td><input name="enumName" class="easyui-validatebox" data-options="required:true" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>枚举值(数字)：</th>
      <td><input name="enumValue" class="easyui-validatebox" data-options="required:true" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>枚举信息(字符串)：</th>
      <td><input name="enumInfo" class="easyui-validatebox" data-options="" style="width:200px;"/></td>
    </tr>
  </table>
  <input name="enumId" type="hidden"/>
</form>

