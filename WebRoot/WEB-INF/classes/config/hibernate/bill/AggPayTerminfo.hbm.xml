<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hrt.biz.bill.entity.model">
    <class name="AggPayTerminfoModel" table="BILL_AGGPAYTERMINFO">
        <id name="bapid" column="BAPID">
            <generator class="sequence">
				<param name="sequence">S_BILL_AGGPAYTERMINFO</param>
			</generator>
        </id>
        <property name="unno" column="UNNO" />
        <property name="mid" column="MID" />
        <property name="Shopname" column="SHOPNAME" />
        <property name="qrtid" column="QRTID" />
        <property name="qrPwd" column="QRPWD" />
        <property name="qrUrl" column="QRURL" />
        <property name="scanRate" column="SCANRATE" />
        <property name="status" column="STATUS" />
        <property name="usedConfirmDate" column="USEDCONFIRMDATE" type="timestamp"/>
        <property name="approveDate" column="APPROVEDATE" type="timestamp"/>
        <property name="maintainDate" column="MAINTAINDATE" type="timestamp"/>
        <property name="maintainUserId" column="MAINTAINUSERID" />
        <property name="approveId" column="APPROVEUID" />
        <property name="minfo1" column="MINFO1" />
        <property name="minfo2" column="MINFO2" />
        <property name="orderid" column="ORDERID" />
        <property name="settlement" column="SETTLEMENT" />
        <property name="secondRate" column="SECONDRATE"/>
    </class>
</hibernate-mapping>
