<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<div class="easyui-layout" data-options="fit:true, border:false">
	<div data-options="region:'north',border:false"
		style="height: 300px; padding-top: 10px;">
		<form id="sysAdmin_merchantterminalinfo_editForm"
			style="padding-left: 5%;">
			<fieldset style="width:920px;">
				<legend>
					装机信息 
				</legend>
				<table class="table1">
					<tr>
						<th>
							bmtid：
						</th>
						<td>
							<input type="text" readonly="readonly" name="bmtid"
								style="width: 150px;" maxlength="200">
						</td>
						<th>
							终端：
						</th>
						<td>
							<input type="text" id="tid" name="tid" readonly="readonly"  style="width: 150px;"
								maxlength="200">
						</td>
					</tr>
					<tr>
						<th>
							机型：
						</th>
						<td>
							<input type="text" id="bmaidName"  name="bmaidName"
								readonly="readonly" style="width: 150px;" maxlength="200">
						</td>
						<th>
							装机地址：
						</th>
						<td>
							<input type="text" id="installedAddress" name="installedAddress"
								style="width: 150px;" class="easyui-validatebox"
								data-options="required:true" maxlength="200">
							<font color="red">&nbsp;*</font>
						</td>
					</tr>
					<tr>
						<th>
							装机名称：
						</th>
						<td>
							<input type="text" id="installedName" readonly="readonly"
								name="installedName" style="width: 150px;" maxlength="50">
						</td>
						<th>
							联系人：
						</th>
						<td>
							<input type="text" id="contactPerson" name="contactPerson"
								style="width: 150px;" maxlength="30">
						</td>
					</tr>
					<tr>
						<th>
							联系人电话：
						</th>
						<td>
							<input type="text" id="contactPhone" name="contactPhone"
								style="width: 150px;" maxlength="20" class="easyui-validatebox">
						</td>
						<th>
							报单日期：
						</th>
						<td>
							<input type="text" id="maintainDateStr" readonly="readonly"  name="maintainDateStr"
								style="width: 150px;" maxlength="20">
						</td>
					</tr>
					<tr>
						<th>
							批单日期：
						</th>
						<td>
							<input type="text" id="approveDateStr" readonly="readonly"  name="approveDateStr"
								style="width: 150px;" maxlength="20" class="easyui-validatebox">
						</td>
						<th>
							装机日期：
						</th>
						<td>
							<input type="text" id="approveDateStr" readonly="readonly" name="approveDateStr"
								style="width: 150px;" maxlength="20">
						</td>
					</tr>
				</table>
			</fieldset>
		</form>
	</div>
		<div data-options="region:'center', border:false"
		style="overflow: hidden;">
	<table id="sysAdmin_10486_datagrid"></table>
</div>

