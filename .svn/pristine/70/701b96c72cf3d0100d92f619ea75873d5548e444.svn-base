<%@ page language="java" contentType="text/html; charset=UTF-8"
         pageEncoding="UTF-8" %>

<form id="sysAdmin_term_sim_m_modifyForm1" method="post">
  <table class="table">
    <tr>
      <th>商户编号：</th>
      <td><input name="mid" id="readonly_mid" readonly="readonly" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>SIM卡号：</th>
      <td><input name="sim" id="readonly_sim" readonly="readonly" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>SN号：</th>
      <td><input name="sn" id="readonly_sn" readonly="readonly" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>sn使用状态：</th>
      <td><input name="snStatus" id="readonly_snStatus" readonly="readonly" style="width:200px;"/></td>
    </tr>
  </table>
</form>
<br/>
<hr/>
<form id="sysAdmin_term_sim_m_modifyForm2" method="post">
  <table class="table">
    <tr>
      <th>SIM卡状态：</th>
      <td>
        <select name="status" id="modify_status" style="width:205px;">
          <option value="1">正常</option>
          <option value="2">欠费</option>
          <option value="3">注销</option>
        </select>
      </td>
    </tr>
    <tr>
      <th>测试期结束时间：</th>
      <td><input name="tryDate" id="modify_tryDate" style="width:200px;"/></td>
    </tr>
    <tr>
      <th>续费日期：</th>
      <td><input name="payDate" id="modify_payDate" style="width:200px;"/></td>
    </tr>
  <input type="hidden" name="btsId" id="static_btsId" />
  </table>
</form>
