	public void setHuabei1(BigDecimal huabei1) {
		this.huabei1 = huabei1;
	}
	public BigDecimal getHuabei2() {
		return huabei2;
	}
	public void setHuabei2(BigDecimal huabei2) {
		this.huabei2 = huabei2;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getUnno() {
		return unno;
	}
	public void setUnno(String unno) {
		this.unno = unno;
