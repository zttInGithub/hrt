/**
 * 自定义政策费率配置表
 */
public class SpecialRateConditionModel implements java.io.Serializable {

	private static final long serialVersionUID = 1L;

	private Integer id;
	private String unno;//特殊中心
	private String rebatetype;//特殊活动
