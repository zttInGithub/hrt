	}
	public String getRatetype() {
		return ratetype;
	}
	public void setRatetype(String ratetype) {
		this.ratetype = ratetype;
	}
	public BigDecimal getScanSpecialRate3() {
		return scanSpecialRate3;
	}
	public void setScanSpecialRate3(BigDecimal scanSpecialRate3) {
		this.scanSpecialRate3 = scanSpecialRate3;
	}
