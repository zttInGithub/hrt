<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hrt.biz.check.entity.model">
    <class name="CheckCashbackDayModel" table="CHECK_CASHBACK_DAY">
        <id name="ccdid" column="CDID">
            <generator class="sequence">
				<param name="sequence">S_CHECK_CASHBACK_DAY</param>
			</generator>
        </id>
      <property name="unno" column="UNNO" ></property>
      <property name="unnoName" column="UNNO_NAME" ></property>
      <property name="upper2Unit" column="UPPER2_UNIT" ></property>
      <property name="upper2Name" column="UPPER2_NAME" ></property>
      <property name="upper1Unit" column="UPPER1_UNIT" ></property>
      <property name="upper1Name" column="UPPER1_NAME" ></property>
      <property name="upperUnit" column="UPPER_UNIT" ></property>
      <property name="upperName" column="UPPER_NAME" ></property>
      <property name="mid" column="MID"></property>
      <property name="tid" column="TID"></property>
      <property name="sn" column="SN" ></property>
      <property name="snType" column="SN_TYPE"></property>
      <property name="keyConfirmDate" column="KEYCONFIRMDATE"></property>
      <property name="keyMonth" column="KEYMONTH"></property>
      <property name="usedConfirmDate" column="USEDCONFIRMDATE" type="timestamp"></property>
      <property name="useMonth" column="USEMONTH" ></property>
      <property name="txnMonth" column="TXNMONTH"></property>
      <property name="rebate1Amt" column="REBATE1_AMT" ></property>
