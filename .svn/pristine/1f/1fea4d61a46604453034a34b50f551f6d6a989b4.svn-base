      <property name="rebate2Amt" column="REBATE2_AMT" ></property>
      <property name="rebate2Month" column="REBATE2_MONTH"></property>
      <property name="rebate3Amt" column="REBATE3_AMT"></property>
      <property name="rebate3Month" column="REBATE3_MONTH"></property>
      <property name="txnAmount" column="TXNAMOUNT"></property>
      <property name="txnCount" column="TXNCOUNT"></property>
      <property name="rebateType" column="REBATETYPE"></property>
      <property name="mInfo1" column="MINFO1"></property>
      <property name="mInfo2" column="MINFO2"></property>
      <property name="cDate" column="CDATE" type="timestamp"></property>
      <property name="lmDate" column="LMDATE" type="timestamp"></property>
    </class>
</hibernate-mapping>
