<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
<form id="sysAdmin_machineInfo_editForm" method="post">
	<table class="table">
    	<tr>
    		<th>机具型号：</th>
    		<td><input type="text" name="machineModel" readonly="readonly" data-options="required:true" class="easyui-validatebox" style="width:250px;"/></td>
    	</tr>
    	<tr>
    		<th>机具类型：</th>
    		<td>
    		<select name="machineType" id="machineType">
    		<option value="1">有线(拨号)</option>
    		<option value="2">有线(拨号和网络)</option>
    		<option value="3">无线</option>
    		</select>
    		</td>
    	</tr>
    	<!-- <tr>
    		<th>机具单价：</th>
    		<td>
    			<input type="text" name="machinePrice" class="easyui-numberbox" data-options="required:true" style="width:250px;"/>
    		</td>
    	</tr> -->
    	<tr>
    		<th>机具库存：</th>
    		<td><input type="text" name="machineStock" class="easyui-numberbox" data-options="required:true" style="width:250px;"/></td>
    	</tr>
    </table>
    <input type="hidden" name="bmaID">
    <input type="hidden" name="machinePrice">
</form>